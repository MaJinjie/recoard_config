; Contents of ~/.config/zflai/myfile.def
[access]
engine = file
file = %TABLE%.log
path = %Zdirs[CACHE]%/zflai/

[hooks]
on_open = STATUS: Opening file %TABLE%.log
on_open_sh = print Hello world! >> %Zdirs[CACHE]%/zflai/file_backend.nfo
on_close = STATUS: Closing file %TABLE%.log
on_close_sh = print Hello world! >> %Zdirs[CACHE]%/zflai/file_backend.nfo

; vim:ft=dosini
